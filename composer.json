{
    "type": "project",
    "license": "MIT License",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "nesbot/carbon": "^2.33",
        "spatie/async": "^1.4",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/process": "5.0.*",
        "tightenco/collect": "^7.9"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {

        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.25",
        "fzaninotto/faker": "^1.9"
    }
}
