{
    "type": "project",
    "license": "MIT License",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "nesbot/carbon": "^2.53",
        "spatie/async": "^1.5",
        "symfony/console": "^5.3",
        "symfony/dotenv": "^5.3",
        "symfony/process": "^5.3",
        "tightenco/collect": "^8.34"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {

        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0",
        "fakerphp/faker": "^1.16"
    }
}
